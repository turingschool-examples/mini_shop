<h1>All Merchants</h1>

<section class="item-cards">
  <% @merchants.each do |merchant| %>
    <section class="item-card-single">
    <h2><%= link_to "#{merchant.name}", "/merchants/#{merchant.id}" %></h2>
    </section>
  <% end %>
  <%= link_to 'Create New Merchant', '/merchants/new' %>
</section>
