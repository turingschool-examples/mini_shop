<h1>Create a New Merchant</h1>

<section class="item-cards">
<section class="item-card">
<%= form_tag "/merchants/", class: "form-item" do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>

  <%= label_tag :address %>
  <%= text_field_tag :address %>

  <%= label_tag :city %>
  <%= text_field_tag :city %>

  <%= label_tag :state %>
  <%= text_field_tag :state %>

  <%= label_tag :zip %>
  <%= text_field_tag :zip %>

  <%= submit_tag 'Submit', class: :button %>
<% end %>
</section>
</section>
