<h2>Edit Info for <%= @merchant.name %></h2>
<%= form_tag "/merchants/#{@merchant.id}", method: :patch do %>
  <%= text_field_tag :name %>
  <%= text_field_tag :address %>
  <%= text_field_tag :city %>
  <%= text_field_tag :state %>
  <%= text_field_tag :zip %>
  <%= submit_tag 'Submit' %>
<% end %>
